<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%@ page session="false" %>
<html>
<head>
	<title>Index</title>
</head>
<body>
<h1>
	List of friends that came today  
</h1>
<c:if test="${friendsnames ne null}">
	<p>There are friends in your list!</p>
</c:if>
<c:if test="${friendsnames eq null}">
	<p>There are no friends in your list!</p>
</c:if>
<ul>
	<c:forEach items="${friendsnames}" var="friend">
		<li>${friend}</li>
	</c:forEach>
</ul>
<p>Have a nice day!</p>
</body>
</html>